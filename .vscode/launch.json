{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debugger Python: current file with arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                "${command:pickArgs}"
            ]
        }
    ]
}